{
  "name": "eventlite",
  "dependencies": {
    "date-fns": "1.29.0",
    "delay": "2.0.0",
    "dlv": "1.1.1",
    "p-series": "1.0.0",
    "querystringify": "1.0.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "simple-get-promise": "1.0.2"
  },
  "devDependencies": {
    "dotenv": "5.0.1",
    "microbundle": "0.4.4"
  },
  "scripts": {
    "build": "microbundle --compress=false --sourcemap=false -f cjs --jsx React.createElement -o dist",
    "postbuild": "rm -rf build && mkdir build",
    "poststart": "echo eventlite.tlvince.com > build/CNAME",
    "prestart": "npm run build",
    "start": "node -r dotenv/config dist/eventlite.js > build/index.html"
  },
  "description": "Lightweight Eventbrite",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tlvince/eventlite.git"
  },
  "keywords": [
    "lightweight",
    "eventbrite"
  ],
  "author": "Tom Vincent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tlvince/eventlite/issues"
  },
  "homepage": "https://eventlite.tlvince.com",
  "engines": {
    "npm": "5.0.2",
    "node": "6.10.3"
  }
}
