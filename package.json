{
  "name": "eventlite",
  "description": "Lightweight Eventbrite",
  "version": "2.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint '*.js' 'src/**/*.ts{,x}'",
    "postbuild": "rm -rf build && mkdir build",
    "poststart": "echo eventlite.tlvince.com > build/CNAME",
    "prestart": "npm run build",
    "start": "node -r dotenv/config dist/index.js > build/index.html",
    "test": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tlvince/eventlite.git"
  },
  "keywords": [
    "lightweight",
    "eventbrite"
  ],
  "author": "Tom Vincent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tlvince/eventlite/issues"
  },
  "homepage": "https://eventlite.tlvince.com",
  "engines": {
    "npm": ">=6.12",
    "node": ">=10"
  },
  "dependencies": {
    "date-fns": "2.7.0",
    "delay": "4.3.0",
    "p-series": "2.1.0",
    "querystringify": "2.1.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "simple-get-promise": "2.0.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "8.2.0",
    "@types/node": "12.12.7",
    "@types/react": "16.9.11",
    "commitlint": "8.2.0",
    "dotenv": "8.2.0",
    "eslint": "6.6.0",
    "eslint-config-tlvince": "2.0.9",
    "eslint-plugin-react": "7.16.0",
    "husky": "3.0.9",
    "lint-staged": "9.4.3",
    "prettier": "1.19.1",
    "typescript": "3.7.2"
  }
}
